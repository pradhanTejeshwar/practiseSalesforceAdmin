<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Age_Not_Zero</fullName>
    <active>true</active>
    <description>Age should not be Zero</description>
    <errorConditionFormula>Age__c &lt;= 0</errorConditionFormula>
    <errorDisplayField>Age__c</errorDisplayField>
