<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Rating_Value_1_to_5</fullName>
    <active>true</active>
    <description>Rating Value should be between 1 to 5</description>
    <errorConditionFormula>OR(Rating__c &lt; 1,Rating__c &gt; 5)</errorConditionFormula>
    <errorDisplayField>Rating__c</errorDisplayField>
    <errorMessage>Rating Value should be between 1 to 5</errorMessage>
</ValidationRule>
