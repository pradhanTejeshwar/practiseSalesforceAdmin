<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_Pay_less_than_Max_Pay</fullName>
    <active>true</active>
    <description>Min Pay should not be more than Max Pay</description>
    <errorConditionFormula>Min_Pay__c &gt; Max_Pay__c</errorConditionFormula>
    <errorDisplayField>Min_Pay__c</errorDisplayField>
    <errorMessage>Min Pay should never be more than Max Pay</errorMessage>
</ValidationRule>
